<?xml version="1.0" encoding="utf-8" ?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat Code Coverage">
        <Configuration>
          <Format>cobertura</Format>
          <Exclude>[Rubric.Tests.TestAssembly2*]*</Exclude>
          <Exclude>[Rubric.Tests]*</Exclude>
          <!--<Include>[coverlet.*]*,[*]Coverlet.Core*</Include>
          <IncludeDirectory>../dir1/,../dir2/,</IncludeDirectory>
          <IncludeTestAssembly>true</IncludeTestAssembly>
          <DeterministicReport>false</DeterministicReport>
          <ExcludeByFile>**/dir1/class1.cs,**/dir2/*.cs,**/dir3/**/*.cs,</ExcludeByFile>
          -->
          <SingleHit>false</SingleHit>
          <UseSourceLink>true</UseSourceLink>
          <ExcludeByAttribute>Obsolete,GeneratedCodeAttribute,CompilerGeneratedAttribute</ExcludeByAttribute>
          <SkipAutoProps>true</SkipAutoProps>
        </Configuration>
      </DataCollector>
      <DataCollector friendlyName="Code Coverage" uri="datacollector://Microsoft/CodeCoverage/2.0" assemblyQualifiedName="Microsoft.VisualStudio.Coverage.DynamicCoverageDataCollector, Microsoft.VisualStudio.TraceCollector, Version=11.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a">
        <Configuration>
          <CodeCoverage>
            <ModulePaths>
              <Exclude>
                <ModulePath>Rubric.Tests.dll</ModulePath>
                <ModulePath>xunit.*.dll</ModulePath>
                <ModulePath>system.*.dll</ModulePath>
              </Exclude>
            </ModulePaths>
          </CodeCoverage>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>